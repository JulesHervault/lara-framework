<?xml version="1.0"?>
<joinpoints  root_class="program">

	<joinpoint class="program"/>	
	<joinpoint class="file"/>
	<joinpoint class="decl"/>
	<joinpoint class="class" extends="decl"/>
	<joinpoint class="function" extends="decl"/>
	<joinpoint class="method" extends="function"/>
	<joinpoint class="constructor" extends="method"/>
	<joinpoint class="field" extends="decl"/>
	<joinpoint class="expr"/>
	<joinpoint class="call" extends="expr"/>
	<joinpoint class="memberCall" extends="call"/>
	<joinpoint class="constructorCall" extends="memberCall"/>	
	<joinpoint class="fieldRef" extends="expr"/>
	<joinpoint class="varRef" extends="expr"/>	
	<joinpoint class="varDecl" extends="decl"/>
	<joinpoint class="param" extends="varDecl"/>
	<joinpoint class="type"/>
	<joinpoint class="stmt"/>
	<joinpoint class="if" extends="stmt"/>
	<joinpoint class="switch" extends="stmt"/>
	<joinpoint class="case" extends="stmt"/>
	<joinpoint class="loop" extends="stmt"/>
	<joinpoint class="ternary" extends="stmt"/>
	<joinpoint class="binary" extends="stmt"/>	
	<joinpoint class="try" extends="stmt"/>
	<joinpoint class="catch" extends="stmt"/>
	<joinpoint class="throw" extends="stmt"/>
	<joinpoint class="default" extends="case"/>
	<joinpoint class="break" extends="stmt"/>
	<joinpoint class="continue" extends="stmt"/>
	<joinpoint class="goto" extends="stmt"/>
</joinpoints>
