/**
 * Base class for all LARA join points.
 *
 * @class
 */
var JoinPoint = function(astNode) {
	this.astNode = astNode;
	this.type = 'joinpoint';

};

JoinPoint.prototype.line = function() {
	throw "JoinPoint.line not implemented";
}

JoinPoint.prototype.code = function() {
	throw "JoinPoint.code not implemented";
}

JoinPoint.prototype.instanceOf = function(joinPointType) {
	return joinPointType === "joinpoint";
}
